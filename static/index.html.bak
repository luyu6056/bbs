	$('#fwin_mods').hide();
	showDialog("操作成功","right","提交结果",function(){
		reload();
	})
}
<%if(data.Action==9 || data.Action==10){%>
var forum_types={};
websocket_func[CMD_MSG_WS2U_forum_carlist]=function(res){
		var forumselect='';
		for(var i in res.Catlist){
			var cat=res.Catlist[i];
			forumselect+='<optgroup label="--'+cat.Name+'">'
			for(var ii in cat.Forums){
				var forum=cat.Forums[ii];
				forumselect+='<option value="'+forum.Fid+'">'+forum.Name+'</option>'
				forum_types[forum.Fid]=forum.Threadtypes
			}
			forumselect+='</optgroup>'
		}
