			$('select[name="moveto"]').html(forumselect);
			get_threadtypes(res.Catlist[0].Forums[0].Fid)
		<%}else{%>
			get_threadtypes(<%=data.Fid%>)
		<%}%>
		
	}
	ajax_post(WRITE_MSG_U2WS_forum_carlist())
function get_threadtypes(fid){
	var select='<option value="0">无</option>';
	for(var i in forum_types[fid].Types){
		var type=forum_types[fid].Types[i]
		select+='<option value="'+type.Id+'">'+type.Name+'</option>'
	}
	$('select[name="threadtypeid"]').html(select);
}
