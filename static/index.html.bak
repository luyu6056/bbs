function reload(callback){
	tplname=GET('tpl')
	if(tplname==""){
		tplname="forum_index"
	}
	switch(tplname){
		case "forum_index":
		tpl_load(tplname,WRITE_MSG_U2WS_forum_index,{Gid:GET('Gid')},callback)
		break;
		case "forum":
		page=1
		if(parseInt(GET("Page"))>0){
			page=parseInt(GET("Page"))
		}
		tpl_load(tplname,WRITE_MSG_U2WS_forum,{Fid:GET('Fid'),Filter:GET('Filter'),Typeid:GET('Typeid'),Specialtype:GET('Specialtype'),Orderby:GET("Orderby"),Dateline:GET("Dateline"),Page:page},callback)
		break;
		case "login":
		var access_token='',state='';
		if(GET('access_token')!="") access_token=GET('access_token');
		if(GET('state')!="") state=GET('state');
		if(location.hash!=""){
			var matchaccess_token=location.hash.match(/access_token=([^&]+)/);
			if(matchaccess_token.length==2) access_token=matchaccess_token[1];
			var matchstate=location.hash.match(/state=([^&]+)/);
			if(matchstate.length==2) state=matchstate[1];
		}
		tpl_load(tplname,WRITE_MSG_U2WS_tpl_success,{Type:GET('Type'),access_token:access_token,state:state},callback)
		break;
		case "register":
		tpl_load(tplname,WRITE_MSG_U2WS_tpl_success,{},callback)
		break;
		case "forum_viewthread":
		var obj={};
		 obj.Tid=GET('Tid')
		if(parseInt(GET("Page"))>0){
			obj.Page=parseInt(GET("Page"))
		}
		if(parseInt(GET("Ordertype"))==1){
			obj.Ordertype=1
		}
		if(parseInt(GET("Authorid"))>0){
			obj.Authorid=parseInt(GET("Authorid"))
		}
		if(parseInt(GET("Position"))){
			obj.Position=parseInt(GET("Position"))
		}
		if(GET('Fromuid')!=""){
			obj.Fromuid=GET('Fromuid')
		}
	 
		tpl_load(tplname,WRITE_MSG_U2WS_forum_viewthread,obj,callback);
		break;
		case "forum_newthread":
		var special=GET('Special');
		if(special==""){
			special=0;
		}
		tpl_load(tplname,WRITE_MSG_U2WS_forum_newthread,{Fid:GET('Fid'),Special:GET('Special'),Type:GET('Type'),Tid:GET('Tid'),Position:GET('Position')},callback)
		break;
		case "space":
		tpl_load(tplname,WRITE_MSG_U2WS_space,{Uid:GET('Uid'),Do:GET('Do'),Name:GET('Name')},callback)
		break;
		case "spacecp":
		tpl_load(tplname,WRITE_MSG_U2WS_spacecp,{Ac:GET('Ac'),Gid:GET('Gid'),Token:GET('Token')},callback)
		break;
		case "resetpw":
		tpl_load(tplname,WRITE_MSG_U2WS_tpl_success,{Step:GET('Step'),Email:GET("Email")},callback)
		break;
		case "bindaccount":
		tpl_load(tplname,WRITE_MSG_U2WS_tpl_success,{Type:GET('Type'),Openid:GET("Openid"),State:GET('State'),Access_token:GET("Access_token")},callback);
		break; 
	}
}
