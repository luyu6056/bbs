		});
		obj.Expiration='<%=data.Tid%>'
	break;
	case 13:
		$('input[name="banned"]').each(function(index, el) {
			if($(this).prop('checked')){
				obj.Param=$(this).val();
			}
		});
		obj.Expiration='<%=data.Tid%>'
	break;
	case 14:
		obj.Reason=$('input[name="tags"]').val();
	break;
	case 15:
		$('input[name="stickreply"]').each(function(index, el) {
			if($(this).prop('checked')){
				obj.Param=$(this).val();
			}
		});
		obj.Expiration='<%=data.Tid%>'
	break;
	case 16:
		obj.Expiration='<%=data.Tid%>'
	break;
	}
	
	obj.Sendreasonpm=$('sendreasonpm').checked?1:0;
	if(!obj.Expiration){
		obj.Expiration=0;
	}
	ajax_post(WRITE_MSG_U2WS_threadmod(obj))
	return false;
});
